    "@angular/router": "^20.3.0",
    "@ngneat/until-destroy": "^10.0.0",
    "@ngrx/operators": "^20.1.0",
    "@ngrx/signals": "^20.1.0",
    "@primeuix/themes": "^1.2.5",
    "primeicons": "^7.0.0",
